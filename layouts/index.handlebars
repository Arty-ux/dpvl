<!doctype html>
<html lang="{{langCode}}">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="{{meta_description}}" />
    <meta http-equiv="content-language" content="{{langCode}}" />
    <meta name="google-site-verification" content="8qbz_VyA9hDGtrntffwC0SA3k8ePIbYsh71SUrj8Sns" />
    <title>{{meta_title}}</title>
    <!--[if IE]><link rel="shortcut icon" href="favicon.ico"><![endif]-->
    <link rel="icon" href="favicon.png">
    <link rel="apple-touch-icon-precomposed" href="apple-touch-icon-precomposed.png">

    <link href='https://fonts.googleapis.com/css?family=Oswald:400,300' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" type="text/css" href="/assets/app.css?{{fileHash}}">
</head>
<body data-function="{{function}}" data-spy="scroll" data-target="#scrollspy" data-offset="140">
	<div class="language">
		<div class="container"> 
			<div class="languages">
			    {{#each collections}}
			    	{{#if (not (eq @key ../langCode))}}
			    	<span class="lang-item"><a href="/{{#if (not (eq @key "de"))}}{{@key}}{{/if}}">{{{this.metadata.langLabel}}} <span class="glyphicon glyphicon glyphicon-chevron-up glyphicon-rotate-90"></span></a></span>
			    	{{else}}
			    	<span class="lang-item">{{{this.metadata.langLabel}}} <span class="glyphicon glyphicon-ok"></span></span>
			    	{{/if}}
			    {{/each}}
			</div>
		</div>
	</div>
	<nav class="navbar navbar-default navbar-fixed-top" id="nav">
		<div class="container"> 
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false"> 
					<span class="glyphicon glyphicon-menu-hamburger text-primary"></span>
				</button>
				<a class="navbar-brand" href="#home">{{site_title}}</a>
			</div>
			<div class="collapse navbar-collapse" id="navbar-collapse" aria-expanded="false"> 
				<div class="navbar-right" id="scrollspy"> 
					<ul class="nav navbar-nav"> 
					{{#each contentItems}}
						{{#if (not this.nonav)}}
			            <li class="nav-item">
			            	<a href="#{{this.link}}">{{this.title}}</a>
			            </li>
			            {{/if}}
			        {{/each}}
					</ul> 
				</div> 
			</div> 
		</div> 
	</nav>
    {{#each contentItems}}
        {{#if (not this.tmpl)}}	            
            {{#extend "default"}}
				{{#content "main"}}
                	{{{this.contents}}}
	            {{/content}}

				{{#content "image"}}
	                {{this.image}}
	            {{/content}}
	        {{/extend}}
        {{else}}
            {{#extend this.tmpl}}
				{{#content "main"}}
                	{{{this.contents}}}
	            {{/content}}

				{{#content "image"}}
	                {{this.image}}
	            {{/content}}
	        {{/extend}}
        {{/if}}
	{{/each}}
	<div class="footer">
		<div class="container"> 
			&copy; {{copyright}}
		</div>
	</div>
	<script src="/assets/js/jquery.2.1.3.min.js?{{fileHash}}"></script>
	<script src="/assets/js/bootstrap.3.3.6.js?{{fileHash}}"></script>
	<script src="/assets/js/jquery.touchSwipe.min.js?{{fileHash}}"></script>
	<script src="/assets/js/jquery.debounce.js?{{fileHash}}"></script>
	<script src="/assets/app.js?{{fileHash}}"></script>
	<script>(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o), m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m) })(window,document,'script','https://www.google-analytics.com/analytics.js','ga'); ga('create', 'UA-71274454-1', 'auto'); ga('send', 'pageview');</script>
</body>
</html>